<div style="padding-top:100px">
    <h4>Hello, {{adminname}}</h4>
    <hr>
    <div class="result">
        <table class="table table-striped">
            <thead class="table-dark">
                <tr>
                    <th scope="col" style="width:40px">Userid</th>
                    {{!-- <th scope="col" style="width:60px">Address</th> --}}
                    <th scope="col">BOOK ISBN, Quantity</th>
                    <th scope="col">View Details</th>
                </tr>
            </thead>
            {{#each useOrders}}
            <tbody class="thead-light">
                <td scope="col">{{this._id}}</td>
                <td scope="col">
                    <ul class="list-group">
                        {{#each this.orders}}
                        <li class="list-group-item"><span style="font-weight:bold">ISBN:</span>{{this.ISBN}},&nbsp;
                            <span style="font-weight:bold">Quantity:</span>{{this.quantity}}</li>
                        {{/each}}
                    </ul>
                </td>
                <td scope="col"><a href="{{this._id}}/userOrderCart" class="btn edit"><i
                            class="fa fa-eye" aria-hidden="true" style="color:black;font-weight:bold;"></i></a>
            </tbody>
            {{/each}}
        </table>
    </div>
</div>